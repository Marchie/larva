{
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "prettier"
  ],
  "plugins": [
    "@typescript-eslint"
  ],
  "ignorePatterns": [
    "*.js",
    "!jest.config.js",
    "*.d.ts",
    ".idea",
    "cdk.out",
    "coverage",
    "node_modules"
  ]
}
